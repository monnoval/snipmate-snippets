snippet =defaults
	body {background: #${1:222};}
	div, ul, ol, li, form { position:relative }
	p,ul,ol {margin-bottom: 15px}
	ul, ol {margin: 0 0 15px 25px}
	ul li {list-style-type: circle}
	ol li {list-style-type: decimal}
	a, a:visited {color: #${1:fff}; text-decoration: underline;}
	a:hover {text-decoration: none;}
	hr {clear: both; border: 0 none; border-top: 1px solid #ccc; display: inline-block; margin: 15px 0px; width: 100%;}
snippet =layout
	/* -------------------------------------------------------------- start
		Layout 
			Basic Layout
			Defaults
			Header
			Content
			Footer
			Helpers 

	-----------------------------------------------------------------
	----------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Basic Layout 
	-------------------------------------------------------------- */

	/* Body as wrapper - http://camendesign.com/code/developpeurs_sans_frontieres */
	html {}
	body {/* center */}

	#content {}
	header   {}
	footer   {}
	
	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Defaults
	-------------------------------------------------------------- */

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Header 
	-------------------------------------------------------------- */
	header {}

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Content 
	-------------------------------------------------------------- */
	#content {}

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Footer 
	-------------------------------------------------------------- */
	footer {}
	
	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Helpers 
	-------------------------------------------------------------- */
	/*
	 * Non-semantic helper classes: please define your styles before this section.
	 */

	/* for image replacement */
	.ir { display: block; text-indent: -999em; overflow: hidden; background-repeat: no-repeat; text-align: left; direction: ltr; }

	/* Hide for both screenreaders and browsers
	 	css-discuss.incutio.com/wiki/Screenreader_Visibility */
	.hidden { display: none; visibility: hidden; }

	/* Hide only visually, but have it available for screenreaders: by Jon Neal
	  www.webaim.org/techniques/css/invisiblecontent/  &  j.mp/visuallyhidden */
	.visuallyhidden {
	 	 border: 0;
	 	 clip: rect(0 0 0 0);
	 	 height: 1px;
	 	 margin: -1px;
	 	 overflow: hidden;
	 	 padding: 0;
	 	 position: absolute;
	 	 width: 1px;
	}

	/* Hide visually and from screenreaders, but maintain layout */
	.invisible { visibility: hidden; }

	/* >> The Magnificent CLEARFIX: Updated to prevent margin-collapsing on child elements << j.mp/bestclearfix */
	.clearfix:before, .clearfix:after { content: "\0020"; display: block; height: 0; visibility: hidden; }
	.clearfix:after { clear: both; }
	/* fix clearfix: blueprintcss.lighthouseapp.com/projects/15318/tickets/5-extra-margin-padding-bottom-of-page */
	 .clearfix { zoom: 1; }
	/* for alignment */
	.center {text-align: center}
	.left   {float: left; overflow: hidden}
	.right  {float: right; overflow: hidden}

	/* width options */
	.width25  {width: 25%} 
	.width35  {width: 35%} 
	.width50  {width: 50%} 
	.width65  {width: 65%} 
	.width100 {width: 100%}

	/* option for lists to have no styles */
	ul.nostyle, ol.nostyle {margin: 0}
	ul.nostyle li, ol.nostyle li {list-style-type: none}

	/* for two and three column divisions */
	.divvy2, .divvy3 {display: inline-block; width: 100%}
	.divvy2 .div1, .divvy2 .div2,
	.divvy3 .div1, .divvy3 .div2, .divvy3 .div3 {float: left}
	.divvy2 .div1, .divvy2 .div2 {width: 50%}
	.divvy3 .div1, .divvy3 .div2, .divvy3 .div3 {width: 33.3%}

	/* -------------------------------------------------------------- end */
snippet =typo
	/* -------------------------------------------------------------- start
		Typography 
			Font families 
			Defaults 
			Headings
				Specifics: h2
				Specifics: h3
			Links

	-----------------------------------------------------------------
	----------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Font families 
	-------------------------------------------------------------- */
	body, select, input, textarea, button {font-family: "Helvetica Neue", Helvetica, Arial, "DejaVu Sans Condensed", FreeSans, sans-serif;}

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Defaults 
	-------------------------------------------------------------- */

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Headings 
	-------------------------------------------------------------- */
	h1 {font-size: 300%}
	h2 {font-size: 245%}
	h3 {font-size: 167%}
	h4 {font-size: 153.9%}
	h5 {font-size: 138.5%}
	h6 {font-size: 123.1%}
	h2,h4,h6 {}
		
	/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Specifics: h2 
		-------------------------------------------------------------- */
		
		/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Specifics: h3 
		-------------------------------------------------------------- */
		
		/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Links 
	-------------------------------------------------------------- */

	/* -------------------------------------------------------------- end */
	/* -------------------------------------------------------------- start
			Helpers 
	-------------------------------------------------------------- */

	/* http://developer.yahoo.com/yui/3/cssfonts/ */
	.font10 {font-size: 77%}
	.font11 {font-size: 85%}
	.font12 {font-size: 93%}
	.font13 {font-size: 100%}
	.font14 {font-size: 108%}
	.font16 {font-size: 116%}
	.font18 {font-size: 138%}
	.font20 {font-size: 153.9%}
	.font22 {font-size: 167%}
	.font24 {font-size: 182%}
	.font26 {font-size: 197%}
	.bold {font-weight: bold}

