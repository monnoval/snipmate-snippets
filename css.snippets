snippet =text
	/* -------------------------------------------------------------- start
			Text elements 
	-------------------------------------------------------------- */
	ul { list-style-type: circle; }
	ol { list-style-type: decimal; }

	dl { margin: 0 0 1.5em 0; }
	dl dt { font-weight: bold; }
	dl dd { margin-left: 1.5em; }

	abbr { border-bottom: 1px dotted #000; cursor: help }
	acronym { border-bottom: 1px dashed #000; cursor: help; text-transform: uppercase; }
	address { margin: 0 0 .9em;; font-style: italic; }
	del { color: #000; text-decoration:line-through }
	big { font-size: 130% }
	small { font-size: 85% }
	cite { font-style: italic }
	ins, dfn {border-bottom: 1px solid #666666; font-style: italic; text-decoration: none}

	a { color: #009; text-decoration: none; }
	a:hover { color: #d54e21; text-decoration: underline; }

	blockquote { margin: 0 2.5em; color: #444; font-style: italic}
	blockquote blockquote { color: #888 }
	blockquote p {margin-bottom: 5px;}
	strong { font-weight: bold }
	em { font-style: italic; }
	pre, code { margin: 1.5em 0; white-space: pre; }
	pre, code, tt, samp { font: 1.1em Monaco,monospace,Courier,"Courier New"; line-height: 1.5; } 
	code {background: #eeeeee; padding: 0 3px}
	pre {background: #f9f9f9; border:1px solid #ccc; padding: 9px; font-size: 90%; overflow: auto; border-left: 20px solid #ddd;}
	pre code {background: none }
	tt { display: block; margin: 1.5em 0; line-height: 1.5; }

	/* -------------------------------------------------------------- end */
snippet =style
	/* -------------------------------------------------------------- start
			Styles 
	-------------------------------------------------------------- */
	/* Div and Span styles */
	div.note, span.note         {background-image: url(../images/box-note.png)    } 
	div.info, span.info         {background-image: url(../images/box-info.png)    } 
	div.download, span.download {background-image: url(../images/box-download.png)} 
	div.tip, span.tip           {background-image: url(../images/box-tip.png)     } 
	div.alert, span.alert,
	div.warning, span.warning   {background-image: url(../images/box-alert.png)   } 
	div.note, span.note         {background-color: #eee;    border-top: 1px solid #ccc;	border-bottom: 1px solid #ccc      } 
	div.info, span.info         {background-color: #c5dbed; border-top: 1px solid #8abce7; border-bottom: 1px solid #8abce7} 
	div.download, span.download {background-color: #e7f7d3; border-top: 1px solid #66cc33; border-bottom: 1px solid #66cc33} 
	div.tip, span.tip           {background-color: #fffbbc; border-top: 1px solid #e6db55; border-bottom: 1px solid #e6db55} 
	div.alert, span.alert, div.warning, span.warning  {background-color: #ffd0c3; border-top: 1px solid #e27e62; border-bottom: 1px solid #e27e62}

	div.note, span.note,
	div.info, span.info,
	div.alert, span.alert, div.warning, span.warning,
	div.download, span.download,
	div.tip, span.tip {background-position: 10px 8px; padding: 8px 10px 8px 40px; margin: 10px 0; background-repeat: no-repeat;}

	/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Inline Styles 
		-------------------------------------------------------------- */
		a.icon-folder   {background-image: url(../images/icon-folder.png)} 
		a.icon-file     {background-image: url(../images/icon-file.png)  } 
		a.icon-download {background-image: url(../images/icon-download.png)} 
		a.icon-pdf      {background-image: url(../images/icon-pdf.png)   } 
		a.icon-zip      {background-image: url(../images/icon-zip.png)   } 

		a.icon-folder,
		a.icon-file,
		a.icon-download,
		a.icon-pdf,
		a.icon-zip {background-repeat: no-repeat; background-position: left center; padding: 2px 0 2px 20px}

		/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Unordered List Styles
		-------------------------------------------------------------- */
		ul {margin: 0 0 10px 18px}
		ul.menu {margin: 0}
		ul.links    {list-style-type: none; margin-left: 2px;}
		ul.links li {background-repeat: no-repeat; background-position: 0 5px; padding-left: 18px} 
		ul.links li a {color: #4C6E94;}
		ul.links li a:hover {text-decoration: none; color: #d54e21;}
		.box ul.links {margin-left: 10px; margin-bottom: 5px} 

		ul.check, ul.star {list-style-type: none; margin: 10px 0}
		ul.check li, ul.star li {padding-left: 30px; margin: 5px 0; line-height: 22px}
		ul.check li {background: url(../images/list-check.png) no-repeat}
		ul.star li {background: url(../images/list-star.png) no-repeat}

		/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Ordered List Styles
		-------------------------------------------------------------- */
		ol {margin: 0 0 10px 20px}
		ol.circle {padding-left: 22px; margin-left: 0; line-height: 20px; background: url(../images/list-circle.png) repeat-y}
		ol.circle li {margin-bottom: 4px; font-size: 11px;}

		/* -------------------------------------------------------------- end */
		/* -------------------------------------------------------------- start
				Horizontal Rule
		-------------------------------------------------------------- */
		hr {background: #ccc; border: 0 none; display: block; height: 1px; margin: 15px 0}
		hr.none {height: 0}
		hr.dotted {background: url(../images/hr-dotted.png) repeat-x}
		hr.dotted2 {background: url(../images/hr-dotted2.png) repeat-x; height: 2px;}
		.box div.hr hr {display: none}
		.box div.hr {height: 2px; margin-bottom: 10px}
		/* -------------------------------------------------------------- end */
